{
 "$schema": "https://turborepo.org/schema.json",
 "pipeline": {
  "dev": {
   "dependsOn": ["^prisma:generate"],
   "outputs": []
  },
  "build": {
   "dependsOn": ["^build", "prisma:generate"],
   "outputs": ["dist/**", ".next/**"]
  },
  "deploy": {
   "dependsOn": ["lint", "format:check", "build"],
   "outputs": ["dist/**", ".next/**"]
  },
  "lint": {
   "dependsOn": ["^lint"],
   "outputs": []
  },
  "lint:fix": {
   "dependsOn": ["^lint:fix"],
   "outputs": []
  },
  "format": {
   "dependsOn": ["^format"],
   "outputs": []
  },
  "format:check": {
   "dependsOn": ["^format:check"],
   "outputs": []
  },
  "prisma:generate": {
   "dependsOn": ["^prisma:generate"],
   "cache": false,
   "outputs": ["prisma/schema.prisma"]
  },
  "prisma:migrate": {
   "dependsOn": ["^prisma:migrate"],
   "outputs": ["prisma/migrations/**"]
  },
  "prisma:format": {
   "outputs": []
  }
 },
 "globalEnv": ["CLIENT_ID", "NEXTAUTH_URL", "HOTJAR_ID", "ENVIRONMENT_URL", "TOKEN", "NODE_ENV", "npm_package_version"],
 "globalDependencies": ["pnpm-lock.yaml", "pnpm-workspace.yaml", ".prettierignore", ".prettierrc"]
}
