{
 "$schema": "https://turborepo.org/schema.json",
 "pipeline": {
  "dev": {
   "dependsOn": ["prisma:generate"],
   "outputs": []
  },
  "build": {
   "dependsOn": ["lint", "prisma:generate"],
   "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
  },
  "deploy": {
   "dependsOn": ["build"],
   "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
  },
  "lint": {
   "dependsOn": ["format:check"],
   "outputs": []
  },
  "lint:fix": {
   "outputs": []
  },
  "format": {
   "outputs": []
  },
  "format:check": {
   "outputs": []
  },
  "prisma:generate": {
   "cache": false,
   "outputs": ["prisma/schema.prisma"]
  },
  "prisma:migrate": {
   "outputs": ["prisma/migrations/**"]
  },
  "prisma:format": {
   "outputs": ["prisma/schema.prisma"]
  },
  "pm2": {
   "dependsOn": ["build"]
  }
 },
 "globalEnv": ["CLIENT_ID", "NEXTAUTH_URL", "HOTJAR_ID", "CLIENT_SECRET", "ENVIRONMENT_URL", "TOKEN", "NODE_ENV", "RUNTIME", "NEXT_PUBLIC_URL", "npm_package_version"],
 "globalDependencies": [".env", ".prettierignore", ".prettierrc"]
}
