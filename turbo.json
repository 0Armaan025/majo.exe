{
 "$schema": "https://turborepo.org/schema.json",
 "pipeline": {
  "dev": {
   "outputs": []
  },
  "build": {
   "dependsOn": ["^build"],
   "env": ["ANALYZE"],
   "outputs": ["dist/**", ".next/**"]
  },
  "deploy": {
   "dependsOn": ["lint", "format:check", "build"],
   "outputs": ["dist/**", ".next/**"]
  },
  "lint": {
   "outputs": []
  },
  "lint:fix": {
   "outputs": []
  },
  "format": {
   "outputs": []
  },
  "format:check": {
   "outputs": []
  }
 },
 "globalDependencies": ["pnpm-lock.yaml", "pnpm-workspace.yaml", ".prettierignore", ".prettierrc", ".env"],
 "globalEnv": ["TOKEN", "SECRET", "npm_package_version", "NEXTAUTH_URL", "CLIENT_ID", "CLIENT_SECRET", "ANALYZE", "MONGODB_URI", "NODE_ENV", "ENVIRONMENT_URL", "HOTJAR_ID"]
}
