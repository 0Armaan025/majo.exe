{
 "name": "majo.exe",
 "description": "An advanced Discord bot",
 "logo": "https://igorkowalczyk.github.io/lib/favicon/favicon.jpg",
 "repository":"https://github.com/igorkowalczyk/majobot.git",
 "stack": "heroku-20",
 "buildpacks": [
  {
   "url": "heroku/nodejs"
  }
 ],
 "env": {
  "TOKEN": {
   "description": "The bot token (Remember! The TOKEN is super secret)",
   "required": true
  },
  "PREFIX": {
   "description": "The default bot prefix (eg. !majo)",
   "required": true
  },
  "AMEAPI": {
   "description": "Your Amethyste api token",
   "required": true
  },
  "BRAINID": {
   "description": "Your Brainshop AI Brain ID",
   "required": true
  },
  "BRAINKEY": {
   "description": "Your Brainshop AI Brain Key",
   "required": true
  },
  "DOMAIN": {
   "description": "Your website domain (eg https://example.com)",
   "required": false
  },
  "PORT": {
   "description": "Your webiste port, (eg. 8080)",
   "required": false
  },
  "DASHBOARD": {
   "description": "The Web-Dashboard config value. (eg. true/false)",
   "required": true
  },
  "SESSION_SECRET": {
   "description": "Random sequence of words, letterss or numbers",
   "required": true
  },
  "SECRET": {
   "description": "The bot client secret (Remember! The SECRET value is Super-Secret)",
   "required": true
  },
  "ANALYTICS": {
   "description": "oogle analytics tracking ID, used in Web-Dashboard",
   "required": false
  }
 },
 "keywords": [
  "discord",
  "discord-bot",
  "node-js",
  "web-dashboard",
  "bot"
  ]
}
