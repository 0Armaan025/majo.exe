{
 "name": "Majo.exe",
 "description": "Majo.exe have almost everything - Fun, Memes, Images, Giveaway, Economy, Anime and NSFW",
 "logo": "https://user-images.githubusercontent.com/49127376/138303201-7f8db506-b11b-40bf-9b89-3966616b16d5.png",
 "repository": "https://github.com/igorkowalczyk/majo.exe.git",
 "stack": "heroku-20",
 "website": "https://majoexe.ml",
 "success_url": "/",
 "buildpacks": [
  {
   "url": "heroku/nodejs"
  }
 ],
 "env": {
  "TOKEN": {
   "description": "Discord bot token",
   "required": true
  },
  "PREFIX": {
   "description": "Bot prefix (eg. !majo)",
   "required": true
  },
  "AMEAPI": {
   "description": "Amethyste api token",
   "required": true
  },
  "DOMAIN": {
   "description": "Website domain (same as your heroku app name!)",
   "required": true
  },
  "ID": {
   "description": "Discord Bot ID",
   "required": true
  },
  "CONTACT_WEBHOOK": {
   "description": "Contact form webhook URL",
   "required": true
  },
  "STATUS_WEBHOOK": {
   "description": "Status webhook URL",
   "required": true
  },
  "ERRORS_WEBHOOK": {
   "description": "Errors webhook URL",
   "required": true
  },
  "SECRET": {
   "description": "Bot client secret",
   "required": true
  },
  "ANALYTICS": {
   "description": "Google Analytics tracking ID",
   "required": false
  },
  "MYSQL_DATABASE": {
   "description": "MYSQL Database name",
   "required": true
  },
  "MYSQL_HOST": {
   "description": "MYSQL Host Name",
   "required": true
  },
  "MYSQL_PASSWORD": {
   "description": "MYSQL user password",
   "required": true
  },
  "MYSQL_USER": {
   "description": "MYSQL User name (user must have access to database!)",
   "required": true
  }
 },
 "keywords": ["discord", "discord-bot", "node-js", "web-dashboard", "bot", "majoexe"]
}
