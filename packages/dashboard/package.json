  "devDependencies": {
    "@next/bundle-analyzer": "13.1.3",
    "autoprefixer": "10.4.13",
    "compression-webpack-plugin": "10.0.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.3",
    "globby": "13.1.3",
    "lodash": "4.17.21",
    "lodash-webpack-plugin": "0.11.6",
