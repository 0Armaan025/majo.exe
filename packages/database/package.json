{
  "name": "@majoexe/database",
  "version": "6.0.0",
  "description": "Majo.exe main database schema/client/migrations package",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write . --cache --ignore-unknown",
    "format:check": "prettier --check --cache .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:format": "prisma format",
    "prisma:push": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "4.14.1",
    "prisma": "4.14.1"
  },
  "devDependencies": {
   "@igorkowalczyk/eslint-config": "1.3.2",
   "eslint": "8.41.0",
   "eslint-config-turbo": "1.9.9",
   "prettier": "2.8.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "packages/database"
  },
  "keywords": [
    "prisma",
    "database",
    "orm",
    "sql",
    "postgresql",
    "mysql"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme"
}
