{
  "name": "@majoexe/config",
  "version": "6.0.0",
  "description": "Majo.exe shared config for all packages and projects",
  "exports": {
    ".": "./index.js",
    "./bot": "./configs/bot.js",
    "./dashboard": "./configs/dashboard.js",
    "./debugger": "./configs/debugger.js",
    "./global": "./configs/global.js"
  },
  "type": "module",
  "scripts": {
    "format": "prettier . --write --cache --ignore-unknown",
