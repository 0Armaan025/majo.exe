 "dependencies": {
  "@headlessui/react": "1.7.12",
  "@headlessui/tailwindcss": "0.1.2",
  "@heroicons/react": "2.0.16",
  "@igorkowalczyk/is-browser": "3.0.0",
  "@majoexe/util": "workspace:*",
  "@next-auth/mongodb-adapter": "1.1.1",
  "@tippyjs/react": "^4.2.6",
  "@vercel/og": "0.4.1",
  "mongodb": "5.1.0",
  "next": "13.2.4",
  "next-auth": "4.20.1",
  "next-themes": "0.2.1",
