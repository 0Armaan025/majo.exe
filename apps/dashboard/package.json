  "@majoexe/eslint-config": "workspace:*",
  "@next/bundle-analyzer": "13.2.4",
  "autoprefixer": "10.4.14",
  "eslint": "8.36.0",
  "eslint-config-next": "13.2.4",
  "globby": "13.1.3",
  "mini-svg-data-uri": "1.4.4",
  "postcss": "8.4.21",
  "prettier": "2.8.7",
