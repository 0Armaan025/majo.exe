{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "main": "index.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "deploy": "npm run build && npm run start",
    "format": "prettier --write . --cache --ignore-unknown",
    "format:check": "prettier --check --cache .",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "dependencies": {
    "@headlessui/react": "1.7.13",
    "@headlessui/tailwindcss": "0.1.2",
    "@heroicons/react": "2.0.17",
    "@igorkowalczyk/is-browser": "3.1.0",
    "@majoexe/util": "workspace:*",
    "@next-auth/mongodb-adapter": "1.1.1",
    "@tippyjs/react": "^4.2.6",
    "@vercel/og": "0.5.1",
    "autoprefixer": "10.4.14",
    "globby": "13.1.4",
    "mini-svg-data-uri": "1.4.4",
    "mongodb": "5.2.0",
    "next": "13.2.4",
    "next-auth": "4.22.0",
    "next-themes": "0.2.1",
    "node-fetch": "3.3.1",
    "nprogress": "0.2.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-twemoji": "0.5.0",
    "sharp": "0.32.0",
    "swr": "^2.1.2",
    "tailwind-gradient-mask-image": "1.0.0",
    "tailwindcss": "3.3.1",
    "tailwindcss-text-fill": "0.2.0"
  },
  "devDependencies": {
    "@majoexe/eslint-config": "workspace:*",
    "@next/bundle-analyzer": "13.2.4",
    "eslint": "8.38.0",
    "eslint-config-next": "13.2.4",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.7"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ]
}
